{
  "name": "tumblr-webpack",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run clean && npm run watch",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --colors --display-error-details --display-reasons --config webpack.config.js",
    "build": "npm run clean && npm run build:prod",
    "build:dev": "cross-env NODE_ENV=development webpack --progress --colors --config webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.config.js",
    "clean": "rimraf build",
    "lint": "eslint ./webpack.config.js ./src; exit 0"
  },
  "author": "Mustafa ismail",
  "license": "MIT",
  "devDependencies": {
    "append-loader": "^1.0.0",
    "autoprefixer": "^6.5.1",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.16.0",
    "browser-sync": "^2.17.5",
    "browser-sync-webpack-plugin": "^1.0.3",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.0",
    "ejs-loader": "^0.3.0",
    "eslint": "^3.8.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-loader": "0.1.0",
    "file-loader": "^0.9.0",
    "glob": "^7.0.3",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.21.0",
    "imports-loader": "^0.7.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.1.1",
    "postcss-loader": "^1.0.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.0.2",
    "svgo-loader": "^1.1.2",
    "to-string-loader": "^1.1.5",
    "tumblr-theme-parser": "git://github.com/mustafaismail22/tumblr-theme-parser",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "imagesloaded": "^4.1.1",
    "jquery": "^3.1.1",
    "jquery-bridget": "^2.0.1",
    "masonry-layout": "^4.1.1",
    "normalize.css": "^5.0.0"
  }
}
